{
  "name": "plain-license",
  "version": "0.0.0-development",
  "description": "Plain language versions of popular licenses",
  "main": "index.js",
  "scripts": {
    "release": "semantic-release",
    "generate-changelog": "node .github/scripts/generate-changelog.js",
    "prepare": "husky",
    "check": "run-p check:*",
    "check:build": "tsc --noEmit",
    "check:style": "run-p check:style:*",
    "check:style:scss": "stylelint \"src/**/*.scss\"",
    "check:style:ts": "eslint --cache \"src/**/*.ts\"",
    "fix": "run-p fix:*",
    "fix:style": "run-p fix:style:*",
    "fix:style:ts": "eslint --cache \"src/**/*.ts\" --fix",
    "upgrade": "run-s upgrade:*",
    "upgrade:bump": "ncu --upgrade --filterVersion \"/^\\^/\"",
    "upgrade:install": "npm install"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seekinginfiniteloop/PlainLicense.git"
  },
  "keywords": [
    "license",
    "plain language",
    "legal",
    "mkdocs",
    "mkdocs-material"
  ],
  "author": {
    "name": "Adam Poulemanos",
    "email": "adam@plainlicense.org"
  },
  "license": "Plain Unlicense",
  "dependencies": {
    "clipboard": "^2.0.11",
    "escape-html": "^1.0.3",
    "focus-visible": "^5.2.1",
    "fuzzaldrin-plus": "^0.6.0",
    "gsap": "^3.12.5",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.14.0",
    "rxjs": "^7.8.1",
    "tablesort": "^5.3.0",
    "workbox": "^7.1.0"
  },
  "devDependencies": {
    "@asn.aeb/esbuild-css-modules-plugin": "^0.1.9",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@linthtml/linthtml": "^0.10.1",
    "@mdi/svg": "^7.4.47",
    "@primer/octicons": "^19.11.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin-ts": "^2.7.2",
    "@stylistic/stylelint-config": "^2.0.0",
    "@stylistic/stylelint-plugin": "^3.0.1",
    "@types/css-modules": "^1.0.5",
    "@types/escape-html": "^1.0.4",
    "@types/fuzzaldrin-plus": "^0.6.5",
    "@types/html-minifier-terser": "^7.0.2",
    "@types/lunr": "^2.3.7",
    "@types/node": "^22.5.4",
    "@types/resize-observer-browser": "^0.1.11",
    "@types/sass": "^1.45.0",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "autoprefixer": "^10.4.20",
    "chokidar": "^4.0.0",
    "cssnano": "7.0.6",
    "esbuild": "^0.23.1",
    "eslint": "9.10.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsdoc": "^50.2.2",
    "eslint-plugin-no-null": "^1.0.2",
    "github-types": "^1.0.0",
    "gitlab": "^14.2.2",
    "google-fonts-complete": "jonathantneal/google-fonts-complete",
    "html-minifier-terser": "^7.2.0",
    "husky": "^9.1.5",
    "material-design-color": "^2.3.2",
    "material-shadows": "^3.0.1",
    "npm-check-updates": "^17.1.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.45",
    "postcss-dir-pseudo-class": "^9.0.0",
    "postcss-inline-svg": "^6.0.0",
    "postcss-logical": "8.0.0",
    "postcss-pseudo-is": "^0.3.0",
    "preact": "^10.23.2",
    "rimraf": "^6.0.1",
    "sass": "^1.78.0",
    "semantic-release": "^24.0.0",
    "simple-icons": "^13.9.0",
    "stylelint": "^16.9.0",
    "stylelint-config-recess-order": "^5.1.0",
    "stylelint-config-recommended": "^14.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-scss": "^6.5.1",
    "svgo": "^3.3.2",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "uglify": "^0.1.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --edit $1"
    }
  },
  "engines": {
    "node": ">= 18"
  }
}
